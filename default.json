{
  "additionalReviewers": ["xiaofan2406"],
  "branchPrefix": "dependency-",
  "extends": ["config:base"],
  "labels": ["dependencies"],
  "rangeStrategy": "bump",
  "npm": {"stabilityDays": 3},
  "prCreation": "not-pending",
  "schedule": ["every weekend"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^eslint"],
      "groupName": "eslint"
    },
    {
      "matchPackagePatterns": ["redux"],
      "groupName": "redux"
    },
    {
      "matchPackagePatterns": ["^webpack"],
      "groupName": "webpack"
    },
    {
      "matchPackagePatterns": ["^rollup"],
      "groupName": "rollup"
    },
    {
      "matchPackagePatterns": ["^vite", "^@vite"],
      "groupName": "vite"
    },
    {
      "matchPackagePatterns": ["^@testing-library"],
      "groupName": "@testing-library"
    },
    {
      "matchPackagePatterns": ["^@types", "^typescript"],
      "groupName": "typescript"
    }
  ],
  "lockFileMaintenance": {"enabled": true}
}
